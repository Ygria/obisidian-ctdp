{
	"name": "obsidian-tailwindcss-sample-plugin",
	"version": "1.0.0",
	"description": "A sample plugin for Obsidian using TailwindCSS.",
	"main": "main.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:browser dev:obsidian",
		"dev:browser": "vite",
		"dev:obsidian": "cross-env VITE_TARGET_ENV=obsidian vite build --mode development",
		"build": "tsc -noEmit -skipLibCheck && vite build --mode production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@tailwindcss/postcss": "^4.1.12",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.11.18",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.21",
		"builtin-modules": "3.3.0",
		"cross-env": "^7.0.3",
		"npm-run-all": "^4.1.5",
		"obsidian": "latest",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.12",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "^6.0.3"
	},
	"dependencies": {
		"@emoji-mart/data": "^1.2.1",
		"@emoji-mart/react": "^1.1.1",
		"clsx": "^2.1.1",
		"diff": "^8.0.2",
		"emoji-mart": "^5.6.0",
		"js-yaml": "^4.1.0",
		"lottie-react": "^2.4.1",
		"lucide-react": "^0.540.0",
		"nanoid": "^5.1.5",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwind-merge": "^3.3.1"
	}
}
